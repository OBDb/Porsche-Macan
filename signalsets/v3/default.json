{ "commands": [
{ "hdr": "70B", "rax": "775", "fcm1": true, "dbg": true, "cmd": {"22": "18A0"}, "freq": 5,
  "signals": [
    {"id": "MACAN_TID_1", "path": "Tires", "fmt": {           "len": 32, "max": 100,                         "unit": "scalar"  }, "name": "Tire 1 sensor id"},
    {"id": "MACAN_TP_1",  "path": "Tires", "fmt": {"bix": 48, "len": 8,  "max": 25.5, "div": 10,             "unit": "bars"    }, "name": "Tire 1 current pressure", "suggestedMetric": "frontLeftTirePressure"},
    {"id": "MACAN_TT_1",  "path": "Tires", "fmt": {"bix": 56, "len": 8,  "max": 100,             "add": -40, "unit": "celsius" }, "name": "Tire 1 temperature"},
    {"id": "MACAN_TPS_1", "path": "Tires", "fmt": {"bix": 64, "len": 8,  "max": 25.5, "div": 10,             "unit": "bars"    }, "name": "Tire 1 specified pressure"},
    {"id": "MACAN_TPW_1", "path": "Tires", "fmt": {"bix": 72, "len": 8,  "max": 25.5, "div": 10,             "unit": "bars"    }, "name": "Tire 1 warning pressure"},
    {"id": "MACAN_TPOS_1", "path": "Tires", "name": "Tire 1 position", "fmt": {"bix": 32, "len": 8, "map": {
          "0": { "description": "Front left",  "value": "FL" },
          "1": { "description": "Front right", "value": "FR" },
          "2": { "description": "Rear left",   "value": "RL" },
          "3": { "description": "Rear right",  "value": "RR" }
        }}
    }
  ]},
